@import "tailwindcss";

@theme {
  --color-electric-*: initial;
  --color-electric-950: oklch(10% 0.06 285);
  --color-electric-900: oklch(18% 0.1 285);
  --color-electric-800: oklch(25% 0.12 285);
  --color-electric-700: oklch(45% 0.25 285);
  --color-electric-600: oklch(52% 0.24 285);
  --color-electric-500: oklch(60% 0.22 285);
  --color-electric-400: oklch(68% 0.2 285);
  --color-electric-300: oklch(75% 0.18 285);
  --color-electric-200: oklch(82% 0.14 285);
  --color-electric-100: oklch(90% 0.1 285);
}
